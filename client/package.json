    "@baseswapfi/smart-order-router": "^3.15.25",
    "@baseswapfi/v2-sdk": "^1.0.16",
    "@baseswapfi/v3-sdk2": "^3.10.16",
    "@binance-chain/bsc-connector": "^1.0.0",
